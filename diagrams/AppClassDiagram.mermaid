        +getRegion() String
        +getCode() String
        +getCapital() String
        +setContinent(continent) void
        +setRegion(region) void
        +setCode(code) void
        +setCapital(capital) void
        +displayCountires(countries) void
        +mapToCountry(rset) void
        +getCountriesInWorld() ArrayList
        +getCountriesByContinent(continent, N) ArrayList
        +getCountryByRegion(region, N) ArrayList
    }

    class CityReport{
        -country : String
        -district : String
