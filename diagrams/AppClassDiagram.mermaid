        +setDistrict(district) void
        +displayCities(cities) void
        +mapToCity(rset) void
        +getCitiesInWorld(N) ArrayList
        +getCitiesByContinent(continent, N) ArrayList
        +getCitiesByRegion(region, N) ArrayList
        +getCitiesByCountry(country, N) ArrayList
