classDiagram
    class Report{
        -name : String
        -population : int
        -Connection con
        +getName() String
        +getPopulation() int
        #getConnection() Connection
        +setName(name) void
        +setPopulation(population) void
        +connect() void
        +disconnect() void
    }

    class CountryReport{
        -continet : String
        -region : String
        -code : String
        -capital : String
